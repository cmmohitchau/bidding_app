{
  "name": "bidding_app",
  "private": true,
  "scripts": {
    "generate:db" : "cd packages/db && pnpm prisma generate && cd ../.. ",
    "migrate:db" : "cd packages/db && pnpm prisma migrate dev --name init && cd ../.. ",
    "build": "turbo run build",
    "start" : "turbo run start",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
